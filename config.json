{
	"log_level": "info",
	"logging_timestamp": false,
	"disable_web": false,
	"disable_metrics": false,
	"unprotected_metrics": false,
	"username": "http_basic_auth_user_abc",
	"password": "http_basic_auth_password_xyz",
	"s3": {
		"service_label": "dynstrg",
		"bucket_name": "my-database-backups"
	},
	"services": {
		"my_postgres_db": {
			"schedule": "0 0 2,18 * * *",
			"timeout": "2h",
			"retention": {
				"days": 60,
				"files": 250
			}
		},
		"my_mysql_db": {
			"timeout": "35m"
		},
		"other_postgres_db": {
			"timeout": "2h15m"
		},
		"mongodb-for-backend": {
			"schedule": "0 45 0/4 * * *",
			"retention": {
				"files": 500
			}
		}
	},
	"backup_cleaner": {
		"enabled": false,
		"default_end_hours": 30,
		"default_start_hours": 240,
		"outdated_end_hours": 48,
		"outdated_start_hours": 300,
		"group": "running",
		"type": "",
		"instance": "1",
		"es_datehour_pattern": "^.*$"
	}
}
